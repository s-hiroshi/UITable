  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "watchify": "^3.7.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.14.0",
    "jquery": "^3.1.0",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "power-assert": "^1.4.1"
  }
}
