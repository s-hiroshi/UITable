{
  "name": "UITable",
  "version": "0.9.0",
  "description": "This product provides an easy way to create the table by intuitive User Interface.",
  "main": "index.js",
  "scripts": {
    "tests": "mocha --compilers js:babel-register tests/*.test.js",
